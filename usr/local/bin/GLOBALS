# Delay in seconds to wait before paired speaker is disconnected if not default
G_DELAY_SPKR=10

# Delay in second for quick PLAY-PAUSE and NEXT or PREVIOUS transitions
G_DELAY="0.8"

# Set CD read speed: 8 is 8x (tradeoff between drive noise and playback quality)
G_CDSPEED="8" 

# Name for tracks in CD playlist: Localizations!!...
G_TRACK="Track" 
